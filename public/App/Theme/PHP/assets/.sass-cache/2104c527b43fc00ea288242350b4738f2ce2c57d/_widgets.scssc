  }

  &.widget-search { margin-top: 20px;
    form { @include set-input; margin: 0px 0px 20px 0; }
    .content { @include shadow; background-color: $color-light-gray; padding-top: 10px; text-align: center;}
    h2 { padding-bottom: 5px; }
    &:first-child { margin-top: 15px;
      h2 { margin-bottom: 10px; }
    }
  }
  &.properties {
    .image { position: relative;
			a:after { @include transition(opacity .2s linear); background-color: $color-primary; content: ''; display: block; height: 100%; position: absolute; opacity: 0; right: 0px; top: 0px; z-index: 999; width: 100%; }
			a:before { @include transition(opacity .2s linear); color: #fff; content: '\e099'; display: block; font-family: 'PictoPro-Normal'; font-size: 20px; height: 20px; position: absolute; opacity: 0; text-align: center; margin-right: -10px; margin-top: -10px; right: 50%; top: 50%; width: 20px; z-index: 1000; }
      a:hover {
        &:after { @include opacity(.5); }
				&:before { @include opacity(.99); }
      }
    }
  }

  &.agencies {
    .agency { position: relative; font-size: 12px; border-bottom: 1px solid $color-light-gray; margin-bottom: 10px; padding-bottom: 10px; width: 100%;
      .info{
        h2 { display: inline; font-size: 14px; font-weight: bold; margin: 0; padding-bottom: 5px;}
      }
      .number { font-size: 12px; }
      .image { @include shadow; width: 70px; float: left; margin-right: 10px;  }
      .btn { @include white-button; width: auto; margin: 3px 0; }
      .address { margin-bottom: 5px; margin-left: 30px; margin-top: 10px;
        &:before { @include picto-pro; color: $color-secondary; content: "\e063";  font-size: 20px; position: absolute; left: 0; }
      }
      .email { margin-bottom: 5px; margin-left: 30px;
        &:before { @include picto-pro; color: $color-secondary; content: "\e095";  font-size: 20px;  position: absolute; left: 0; }
      }
      .phone { margin-left: 30px;
        &:before { @include picto-pro; color: $color-secondary; content: "\e06a";  font-size: 20px;  position: absolute; left: 0; }
      }
      .url { margin-left: 30px;
        &:before { @include picto-pro; color: $color-secondary; content: "\e0c5";  font-size: 20px;  position: absolute; left: 0; }
      }
      img {
        display: block;
      }
    }
  }

  &.partners {
    .partner { margin-bottom: 20px;
    }
  }
}

.features-grid { background-color: transparent; border-bottom: 1px solid $color-light-gray;
  .content-box { margin-top: 30px;  padding-bottom: 30px;
    .pictopro-icon { @include box-sizing(border-box); background-image: none; border: 2px solid $color-medium-gray; border-radius: 50%; height: 70px;
      .icon { color: $color-primary; font-size: 48px; margin-left: 9px; margin-top: 8px; }

			@media (min-width: 979px) and (max-width: 1199px) {
				height: 60px;
				.icon { background-image: none; background-position: -9999px; display: block; font-size: 42px; height: 56px; line-height: 56px; margin: 0px; text-align: center; width: 56px; }
			}

			@media (min-width: 768px) and (max-width: 979px) {
				height: 42px;
				.icon { background-image: none; background-position: -9999px; display: block; font-size: 30px; height: 38px; line-height: 38px; margin: 0px; text-align: center; width: 38px; }
			}

			@media (max-width: 768px) {
				display: none;
			}
    }
    .content { color: $color-medium-gray; line-height: 20px;
      h3 { color: $color-primary; font-size: 18px; font-weight: normal; line-height: 24px;
				@media (min-width: 768px) and (max-width: 979px) {
					font-size: 14px; line-height: 18px;
				}
			}
			@media (min-width: 768px) and (max-width: 979px) {
				p { font-size: 12px; line-height: 16px; }
			}
      a { color: $color-dark-gray;
        i { color: $color-medium-gray; font-size: 20px; line-height: 15px; margin-left: 5px; text-decoration: none; }
      }
    }
  }
}



.property { @include shadow; background-color: $color-light-gray; margin-bottom: 0px; position: relative; padding: 10px;
  .image { float: left; margin-right: 10px; width: 30%; background-color: $color-primary; }
  .wrapper{ margin-bottom: 5px;
    .title{
      h3 { font-size: 12px; font-weight: bold; line-height: 1; margin: 0;
        a { color: $color-primary; }
      }
    }
    .location { color: $color-dark-gray; font-size: 12px; margin-bottom: 4px;}
    .price { @include shadow; color: $color-dark-gray; background-color: #fff; border-radius: 2px; display: inline; font-size: 12px; font-weight: bold; padding: 4px 8px 4px 8px; }
  }
}
@media (min-width: 1199px) {
  .property {
    .image { float: left; width: 100px; }
    .wrapper { margin-bottom: 5px;
      .title {
        h3 { font-size: 14px; }
      }
    .location { color: $color-dark-gray; font-size: 12px; margin-bottom: 8px;}
    .price {  font-size: 14px; font-weight: bold; padding: 6px 10px 6px 10px; }
    }
  }
}

.property-info { @include shadow; background-color: #fff; font-size: 12px; padding: 2px 10px 2px 5px; margin-bottom: 10px; z-index: 0;
  .area { float: left; }
  .bedrooms { float: right; }
  .bathrooms { float: right; }
  .icon { color: $color-medium-gray; font-size: 16px; line-height: 15px; margin-right: 5px; margin-left: 5px }
}

@media (min-width: 1199px) {
  .property-info { font-size: 12px; padding: 6px 10px 5px 5px;
    .icon { color: $color-medium-gray; font-size: 20px; line-height: 13px; margin-right: 5px; margin-left: 5px }
  }
}

.our-agents {
  .content {
    .agent { border-bottom: 1px solid $color-light-gray; line-height: 12px; margin-bottom: 10px; padding-bottom: 10px;
      &:last-child { margin-bottom: 0px; }
      .image { float: left; margin-right: 10px; width: 50px; }
      .name { margin-bottom: 5px;
        a { color: $color-primary; font-size: 12px; font-weight: bold; }
      }
      .phone, .email { font-size: 12px;
      a { color: $color-dark-gray; text-decoration: underline; }
      }
      .icon { color: $color-secondary; font-size: 12px; margin-right: 0px; }
    }
  }
}
@media (min-width: 1199px) {
  .our-agents {
    .content {
      .agent { line-height: 20px; margin-bottom: 20px; padding-bottom: 20px;
        .image { width: 70px; }
        .name { margin-bottom: 5px;
          a { font-size: 14px; }
        }
        .icon { color: $color-secondary; font-size: 15px; margin-right: 5px; }
      }
    }
  }
}

.login {
  .content {@include shadow; background-color: $color-light-gray; text-align: center;
    form { @include set-input;
      label { color: $color-dark-gray; font-size: 14px; font-weight: bold; background-color: transparent; margin-bottom: 0px; padding-top: 10px; text-align: left;  }
    }
    .control-group { margin-bottom: 0px; }
  }
}

.register {
  .content {@include shadow;  background-color: $color-light-gray; text-align: center;
    form { @include set-input;
      label { color: $color-dark-gray; font-size: 14px; font-weight: bold; background-color: transparent; margin-bottom: 0px; padding-top: 10px; text-align: left;  }
    }
    .control-group { margin-bottom: 0px; }
  }
}


.sidebar {
	.property-filter {
		.chzn-container { width: 100% !important; }
	}
}

.widget {
	.carousel {
		.bx-wrapper .bx-prev { background-image: url('../img/controls-prev.png') !important; background-position: center center !important; background-repeat: no-repeat !important; background-size: 25px 25px !important;
			@media (-webkit-min-device-pixel-ratio: 2), (-moz-min-device-pixel-ratio: 2) {
				background-image: url('../img/controls-prev@2x.png') !important;
			}
		}

		.bx-wrapper .bx-next { background-image: url('../img/controls-next.png') !important; background-position: center center !important; background-repeat: no-repeat !important; background-size: 25px 25px !important;
			@media (-webkit-min-device-pixel-ratio: 2), (-moz-min-device-pixel-ratio: 2) {
				background-image: url('../img/controls-next@2x.png') !important;
			}
		}
	}
