									@media (-webkit-min-device-pixel-ratio: 2), (-moz-min-device-pixel-ratio: 2) {
										background-image: url(../img/property-types-controls-next@2x.png);
									}
								}
								&.bx-prev { left: -9px; background-image: url(../img/property-types-controls-prev.png); background-position: center center; background-repeat: no-repeat; background-size: 26px 26px; opacity: 1; width: 26px;
									@media (-webkit-min-device-pixel-ratio: 2), (-moz-min-device-pixel-ratio: 2) {
										background-image: url(../img/property-types-controls-prev@2x.png);
									}
								}
							}
						}
					}
				}

                input[type=text] { display: inline-block; height: 35px; margin: 1px 0px 0px 0px; width: 100px; }

				select { float: left; width: auto;
					&.location { width: 140px;}
					&.type { width: 140px;}
					&.beds {width: 110px }
					&.baths {width: 110px }
					&.price-from {width: 120px }
					&.price-to {width: 120px }

					@media (min-width: 768px) and (max-width: 979px) {
						&.location { width: 110px; }
						&.type { width: 110px; }
						&.price-from { width: 110px; }
						&.price-to { width: 110px; }
						&.beds { width: 90px; }
						&.baths { width: 90px; }
					}
				}

				@media (max-width: 767px) {
					.chzn-container { margin-bottom: 10px; float: left; width: 48% !important;
						&#inputBeds__chzn { float: right !important; }
						&#inputPriceFrom__chzn { float: right !important; }
						&#inputPriceTo__chzn { margin-bottom: 0px; }
					}

					.btn { float: right !important; margin-bottom: 0px; padding-left: 0px !important; padding-right: 0px !important; width: 48% !important; }
				}

				.btn { @include secondary-button; float:right; padding: 8px 50px; width: auto; margin: 0;
