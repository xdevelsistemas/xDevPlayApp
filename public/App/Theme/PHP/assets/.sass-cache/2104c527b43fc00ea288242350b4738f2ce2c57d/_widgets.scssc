:@template"ÄJ.widget { background-color: $color-light-gray;
  form { margin: 0px; }
  h2 { background-color: white; color: $color-primary; font-size: 14px; font-weight: normal; line-height: 1; margin: 15px 0 5px 0; padding-bottom: 10px; }
  ul { list-style: none; margin: 0px;
    li {  //padding: 0px 0 5px 10px;
      a { color: $color-link; }
    }
  }
  label { background-color: #fff; color: $color-primary; font-size: 18px; }
  .btn { @include primary-button; }
  .post-date { color: $color-medium-gray; font-size: 12px; }
  .form-actions { background-color: transparent; border: none; margin: 0px; padding: 0 0 0px 0; }
  @media (min-width: 1199px) {
    h2 { font-size: 18px; }
    .btn { @include primary-button; }
  }

  .sub-menu {
    ul{
      li { padding-left: 10px; padding-bottom: 0px;
        ul {  }
      }
    }
  }
  .recentcomments {
    .url { font-weight: bold; }
  }

  &.properties, &.our-agents, &.login, &.register, &.widget-search, &.widget-recent_entries, &.widget-recent_comments,
  &.widget-archive, &.widget-categories, &.widget-meta, &.widget-pages, &.widget-tag_cloud, &.widget-calendar,
  &.widget-nav_menu, &.enquire, &.widget-rss,&.partners, &.agencies
  { background-color: transparent; border: none;}

  &.widget-recent_entries, &.widget-recent_comments, &.widget-archive, &.widget-categories, &.widget-meta, &.widget-pages, &.widget-calendar, &.widget-rss, &.agencies {
    ul { @include shadow; background-color: $color-light-gray; padding: 10px 10px 5px 10px;
      li { border-bottom: 1px solid #fff; margin-bottom: 5px; padding-bottom: 5px;
        .post-date { border-bottom: 0px solid #fff; display: block; font-size: 11px; margin-right: 10px; }
        &:last-child {
          .post-date { border-bottom: 0px;}
        }
        &:last-child { border-bottom: 0px;}
      }
    }
  }
  @media (min-width: 1199px) {
    &.widget-recent_entries, &.widget-recent_comments, &.widget-archive, &.widget-categories, &.widget-meta, &.widget-pages, &.widget-calendar {
      ul { padding: 15px 20px 5px 20px; }
    }
  }

  &.widget-pages {
    ul {
      li {
        ul { box-shadow: none; }
      }
    }
  }

  &.widget-nav_menu {
    ul { @include shadow; background-color: $color-light-gray; padding: 3px 4px 2px 0px;
      li { padding-left: 0px !important; margin-top: 5px; border-bottom: 0px !important;
        ul { box-shadow: none; }
      }
    }
    .menu {
      ul {
        li { padding-left: 0px; padding-bottom: 0px;
          a { @include opacity(.6); font-weight: normal; padding-bottom: 5px;  }
          ul {
            li {
            a { color: $color-dark-gray; font-size: 12px; line-height: 14px; }
            }
          }
        }
      }
      a { color: $color-primary; display: block; font-weight: normal; margin-right: 10px; padding-bottom: 5px;
        &:last-child { border-bottom: 0px; }
      }
    }
  }
  @media (min-width: 1199px) {
    &.widget-nav_menu {
      ul { padding: 5px 8px 4px;
        li { padding-left: 10px; }
      }
    }
  }

  &.enquire {
    .content {@include shadow;  background-color: $color-primary; text-align: center;
      .btn { @include secondary-button; }
      form { @include set-input;
        label { color: #fff; font-size: 14px; font-weight: bold; background-color: transparent; margin-bottom: 0px; padding-top: 10px; text-align: left;  }
      }
      .form-actions { background-color: transparent; border: none; margin: 0px; padding: 0px; }
      input[type="submit"] { margin-bottom: 0px; }
      .control-group { margin-bottom: 0px; }
      #inputDate { padding-bottom: 6px; padding-top: 6px; }
      #inputMessage { min-height: 100px; }
    }
    @media (min-width: 1199px) {
      .content {
        .btn { @include secondary-button; }
      }
    }
  }

	&.property-carousel { background-color: #fff;
		h2 { border-bottom: 0px; }
	}

  &.property-filter { @include shadow; background-color: $color-light-gray; margin: 0px !important; padding: 15px 20px 0px 20px;
		@media (max-width: 767px) {
			margin: 0px 20px;
		}
    .control-label {  background-color: transparent; color: $color-dark-gray; font-size: 14px; font-weight: bold; }
    .rent, .sale { display: inline-block; margin-bottom: 5px;  margin-top: 5px; width: 50%; }
    .rent { float: left; }
    .sale { float: right; }
		.checkbox { padding-left: 0px; }

    label { background-color: transparent; color: $color-dark-gray; font-size: 14px; font-weight: bold; }
    .form-actions { text-align: center;
      .btn { @include primary-button; }
    }
    select { width: 100%; }
    .beds {
      &.control-group { clear: left; float: left; width: 48%;
        .controls {
          select { width: 100%; }
          .chzn-container { @include box-sizing(border-box); width: 100% !important; }
        }
      }
    }

    .baths {
      &.control-group { clear: right; float: right; width: 48%;
        .controls {
          select { width: 100%; }
          .chzn-container { @include box-sizing(border-box); width: 100% !important; }
        }
      }
    }

		&.filter-horizontal { position: relative;
			.content {  bottom: 0px; background-color: $color-primary; position: absolute; left: 0; padding: 25px 0; width: 100%;
				form {  padding: 0px 20px;
					.bx-wrapper { @include shadow(); background-color: #fff; left: 0px; max-width: inherit !important; position: absolute; top: -80px; z-index: 1;
						.property-types {

							.property-type { @include transition(background-color .2s); background-repeat: no-repeat; background-position: 12% 50%; border-right: 1px solid $color-light-gray; height: 38px; padding: 15px; position: relative; width: 38px;
								&:before { @include transition(opacity .2s); @include opacity(0); @include picto-pro; color: $color-secondary; content: "\e12b"; font-size: 20px; left: 14px; top: 8px; position: absolute; }
								&:after { @include opacity(.5); @include transition(background-color .6s); @include transition(opacity .2s linear); background-color: $color-medium-gray; border-radius: 50%; color: $color-medium-gray; content: ""; display: block; height: 32px; margin-left: 9px; top: 18px; position: absolute; width: 32px; z-index: -1; }
								&:hover {  background-color: rgba(0,0,0,0.05); cursor: pointer;
									&:after { @include opacity(.8); }
								}

								label { @include transition(opacity .2s); @include opacity(.5); color: $color-dark-gray; font-weight: normal; margin-top: 9px; margin-left: 35px; }

								&.active {
									&:before { @include opacity(1); }
									&:after { @include opacity(1); background-color: $color-primary; }
									label { @include opacity(1); }
								}							

                background-image: url('../img/property-types/family-house.png'); background-size: 38px 38px;
